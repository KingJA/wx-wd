/* pages/search/search.wxss */
@import "../../templates/question-list-template/question-list-template.wxss";
.search-container{
display: flex;
flex-direction: row;
align-items: center;
background-color: #F5F6F8;
border-radius: 40rpx;
padding: 20rpx 40rpx;
margin: 10rpx 40rpx 10rpx;
}

.search-container image{
  width: 32rpx;
  height: 32rpx;
}

.search-container input{
  flex: 1;
  margin-left: 20rpx;
  font-size: 25rpx;
  color: #666;
}