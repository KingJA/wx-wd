<template name="questionItemTemplate">
	<view class="question-item" catchtap="onQuestionDetail" data-question-id="{{question.questionId}}">
		<view class="question-text">
			<text class="question-title">{{question.title}}</text>
			<text class="question-content">{{question.content}}</text>
		</view>
		<!-- <image class="question-img" src="/images/question/q_1.jpg" /> -->
		<image class="question-img" src="{{question.resUrl}}" wx:if="{{question.resUrl}}" />
	</view>

</template>